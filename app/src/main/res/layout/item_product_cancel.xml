<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="105dp"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/no_tv"
        style="@style/tableTitleStyle"
        android:layout_width="105dp"
        android:layout_height="30dp"
        android:background="@drawable/textview_border_bg_gray"
        />

    <TextView
        android:id="@+id/quote_number_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/product_name_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/count_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/product_params_tv"
        style="@style/tableContentStyle"
        android:maxLines="2"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/elevator_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/project_state_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/payment_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/reason_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />
    <TextView
        android:id="@+id/solution_tv"
        style="@style/tableContentStyle"
        android:textSize="@dimen/small_font"
        android:text="column1"
        />

</LinearLayout>